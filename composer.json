{
    "name": "sanmanto-sds/laravel-query-log",
    "type": "library",
    "description": "Laravel sql query logging library.",
    "keywords": [
        "laravel",
        "sql",
        "query",
        "log"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "dev",
            "email": "dev@sanmanto.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "illuminate/database": ">=8.0",
        "illuminate/log": ">=8.0",
        "illuminate/support": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "SanmantoSds\\LaravelQueryLog\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SanmantoSds\\LaravelQueryLog\\ServiceProvider"
            ]
        }
    }
}
